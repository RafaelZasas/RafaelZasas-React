{
  "name": "raf-website-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:tailiwind": "tailwind -o styles/main.css --watch",
    "dev:nextjs": "next dev",
    "emulators": "firebase emulators:start --import ./emulators-data --export-on-exit ./emulators-data",
    "build": "npm run build:css && next build",
    "serve": "next start",
    "lint": "next lint",
    "export": "next export",
    "build:css": "tailwind -o styles/main.css",
    "prod:build": "NODE_ENV=production npm run build",
    "deploy": "npm run build:css && next build && next export"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@fortawesome/fontawesome-pro": "6.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-duotone-svg-icons": "6.1.1",
    "@fortawesome/pro-light-svg-icons": "6.1.1",
    "@fortawesome/pro-regular-svg-icons": "6.1.1",
    "@fortawesome/pro-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-thin-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.4",
    "@iconscout/unicons": "^4.0.0",
    "@tailwindcss/typography": "^0.4.1",
    "dayjs": "1.11.0",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "firebase": "^9.6.9",
    "firebase-functions": "^3.15.3",
    "lodash.debounce": "^4.0.8",
    "next": "^11.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "5.0.3",
    "react-pdf": "^5.3.2",
    "react-spinners": "^0.11.0"
  },
  "devDependencies": {
    "@types/draft-convert": "^2.1.4",
    "@types/draft-js": "^0.11.9",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "autoprefixer": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.3.5"
  }
}
